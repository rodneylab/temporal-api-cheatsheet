{
  "name": "temporal-api-cheatsheet",
  "version": "0.0.1",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "Temporal API Cheatsheet: a quick guide to new JavaScript Temporal API including code snippets and example use cases.",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro telemetry disable && astro build && subfont -ir --root dist",
    "preview": "astro preview",
    "check": "astro check --fail-on-warnings --ignore src/content/blog/**",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint:css": "stylelint \"src/**/*.{astro,css,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@js-temporal/polyfill": "^0.4.2",
    "astro": "^1.0.0-rc.2",
    "astro-icon": "^0.7.3",
    "commitlint": "^17.0.3",
    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.1.2",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "subfont": "^6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/temporal-api-cheatsheet"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/temporal-api-cheatsheet/issues"
  }
}
