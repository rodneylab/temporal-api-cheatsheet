{
	"name": "temporal-api-cheatsheet",
	"version": "0.0.1",
	"private": true,
	"license": "BSD-3-Clause",
	"description": "Temporal API Cheatsheet: a quick guide to new JavaScript Temporal API including code snippets and example use cases.",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro telemetry disable && astro build && subfont -ir --root dist",
		"preview": "astro preview",
		"check": "astro check --fail-on-warnings --ignore src/content/blog/**",
		"format": "prettier --write --plugin=prettier-plugin-astro .",
		"lint:css": "stylelint \"src/**/*.{astro,css,svelte}\"",
		"prettier:check": "prettier --check --plugin=prettier-plugin-astro ."
	},
	"devDependencies": {
		"@astrojs/check": "^0.3.1",
		"@commitlint/config-conventional": "^18.4.0",
		"@js-temporal/polyfill": "^0.4.4",
		"astro": "^3.5.0",
		"astro-icon": "^0.8.1",
		"commitlint": "^18.4.0",
		"dotenv": "^16.3.1",
		"husky": "^8.0.3",
		"postcss-html": "^1.5.0",
		"prettier": "^3.0.3",
		"prettier-plugin-astro": "^0.12.1",
		"stylelint": "^15.11.0",
		"stylelint-config-recommended": "^13.0.0",
		"subfont": "^7.1.1",
		"typescript": "^5.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/temporal-api-cheatsheet"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/temporal-api-cheatsheet/issues"
	}
}
