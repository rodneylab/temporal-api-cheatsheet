{
  "name": "temporal-api-cheatsheet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro telemetry disable && astro build && subfont -ir --root dist",
    "preview": "astro preview",
    "check": "astro check --fail-on-warnings --ignore src/content/blog/**",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint:css": "stylelint \"src/**/*.{astro,css,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@js-temporal/polyfill": "^0.4.1",
    "astro": "^1.0.0-beta.52",
    "astro-icon": "^0.7.1",
    "commitlint": "^17.0.2",
    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.0.12",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "subfont": "^6.5.0"
  }
}
